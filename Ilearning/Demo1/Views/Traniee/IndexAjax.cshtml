@model List<Traniee>
@{
    ViewData["Title"] = "indexAjax";
    Layout = "_AdvancedLayout1";
}


<div id="TranieeID" style="border: 1px solid black; color:black; background-color:darkgray; padding: 10px; margin-top: 20px;">
    This is Names of Traniees
</div>
<table class="table table-bordered table-hover" style="background-color :lightgray ;">
    @foreach (var item in Model)
    {
        <tr>
            <td><a href="#" onclick="GetData(@item.Id); return false;">@item.Name</a>

        </td>
        </tr>
    }
</table>

<a style="" href="/Traniee/New" >New Traniee</a>


<script>
    function GetData(TranieeID) {
        console.log(TranieeID);
        event.preventDefault();
        $.ajax({
            url: "/Traniee/DetailsAjax/" + TranieeID,
            success: function (result) {
                console.log(result);
                document.getElementById("TranieeID").innerHTML = result;
            }

        });
    }
</script>

<partial name="_ScriptPartial" />